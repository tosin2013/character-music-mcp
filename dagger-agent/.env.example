# DeepSeek API Configuration (REQUIRED for all operations)
DEEPSEEK_API_KEY=your_deepseek_api_key

# GitHub Configuration (REQUIRED only for GitHub integration/CI)
# Either use a personal access token or GitHub App credentials
# GITHUB_TOKEN=your_github_token_here
# OR use GitHub App credentials
# GITHUB_APP_ID=your_github_app_id
# GITHUB_PRIVATE_KEY=your_github_private_key
# GITHUB_WEBHOOK_SECRET=your_webhook_secret
DEEPSEEK_MODEL=deepseek-coder
DEEPSEEK_MAX_TOKENS=4000
DEEPSEEK_TEMPERATURE=0.1

# Agent Behavior Configuration
ENABLED_FIX_TYPES=syntax,import,format,test,coverage
PYTHON_VERSIONS=3.10,3.11,3.12
MAX_CONCURRENT_FIXES=3
COVERAGE_THRESHOLD=80.0

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Dagger Configuration
DAGGER_ENGINE_VERSION=latest
DAGGER_CACHE_ENABLED=true
DAGGER_PARALLEL_EXECUTION=true
DAGGER_CONTAINER_TIMEOUT=600